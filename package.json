{
  "name": "express-es6-starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/server.bundle.js",
    "serve": "run-p dev:client:serve dev:server:prepare dev:server:build dev:server:delay",
    "build": "webpack --config webpack.config.babel.js --colors --display-error-details",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.dev.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "delay": "node ./node_modules/npm-delay 5000",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./dist/server",
    "dev:client:serve": "babel-node ./webpack/webpack-dev-server.js",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seeed"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "bootstrap": "4.0.0-beta.2",
    "cookie-parser": "~1.4.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.10.0",
    "express": "~4.15.5",
    "font-awesome": "^4.7.0",
    "hbs": "^4.0.1",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.1.0",
    "minilog": "^3.1.0",
    "morgan": "~1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "reactstrap": "^5.0.0-alpha.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.4.5",
    "sqlite3": "^3.1.13",
    "styled-components": "^2.2.3",
    "universal-webpack": "^0.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap-loader": "^2.2.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
