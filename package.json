{
  "name": "express-es6-starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/server.bundle.js",
    "serve": "run-p dev:client:serve dev:server:prepare dev:server:build dev:server:delay",
    "build": "webpack --config webpack.config.babel.js --colors --display-error-details",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "dev:server:build": "webpack --config ./webpack/webpack.config.server.dev.babel.js --watch --colors --display-error-details",
    "dev:server:delay": "run-s delay dev:server:start",
    "delay": "node ./node_modules/npm-delay 1000",
    "dev:server:start": "nodemon ./src/server/start.babel.js --watch ./dist/server",
    "dev:client:serve": "babel-node ./webpack/webpack-dev-server.js"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.10.0",
    "express": "~4.15.5",
    "hbs": "^4.0.1",
    "minilog": "^3.1.0",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.4.5",
    "universal-webpack": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.2.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "nodemon": "^1.12.1",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
